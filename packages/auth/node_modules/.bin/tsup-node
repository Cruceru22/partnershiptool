#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules/tsup/dist/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules/tsup/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules/tsup/dist/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules/tsup/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/tsup@8.3.5_jiti@2.3.3_postcss@8.4.47_typescript@5.8.2_yaml@2.6.0/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
