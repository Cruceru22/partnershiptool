#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/dist/cli/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/dist/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/dist/cli/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/dist/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules/react-email/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/react-email@3.0.7_@opentelemetry+api@1.9.0_bufferutil@4.0.8_react-dom@19.0.0_react@19.0_4c18b1ac0ae939e2f2d167b5de606b8e/node_modules:/Users/andreicruceru/partnership-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/cli/index.js" "$@"
fi
