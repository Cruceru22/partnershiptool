{
  "name": "@acme/validators",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./limits": {
      "types": "./dist/limits.d.ts",
      "default": "./src/limits.ts"
    },
    "./plans": {
      "types": "./dist/plans.d.ts",
      "default": "./src/plans.ts"
    },
    "./message": {
      "types": "./dist/message.d.ts",
      "default": "./src/message.ts"
    },
    "./redis": {
      "types": "./dist/redis.d.ts",
      "default": "./src/redis.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@upstash/redis": "^1.34.5",
    "human-id": "^4.1.1",
    "ts-pattern": "^5.6.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}
